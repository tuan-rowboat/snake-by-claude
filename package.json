{
  "name": "snake-game-vue3",
  "version": "1.0.0",
  "description": "Snake Game converted from React to Vue 3 with Composition API",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
