    "@vue/tsconfig": "^0.5.0",
    "autoprefixer": "^10.4.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.0.0",
    "vite": "^7.1.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.0.0"
  }
